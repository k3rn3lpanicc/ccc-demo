# Example environment file for private key configuration
# Copy this to .env and fill in your values

# Admin private key (for contract listener and finish script)
# This should be the contract admin/deployer key (without 0x prefix)
ADMIN_PRIVATE_KEY=your_admin_private_key_here

# Alternative: PRIVATE_KEY (used if ADMIN_PRIVATE_KEY not set)
PRIVATE_KEY=your_private_key_here

# RPC URL for BSC Testnet
RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545

# Contract addresses (will be auto-filled after deployment)
CONTRACT_ADDRESS=
TOKEN_ADDRESS=
